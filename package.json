{
  "name": "rut-status-cli",
  "version": "2.0.0",
  "description": "Check rut status in registro civil CLI",
  "main": "src",
  "scripts": {
    "prepublish": "eslint src"
  },
  "engines": {
    "node": ">=4"
  },
  "bin": {
    "rut-status": "src/index.js"
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/lgaticaq/rut-status-cli.git"
  },
  "keywords": [
    "rut",
    "cli"
  ],
  "author": "Leonardo Gatica <lgatica@protonmail.com> (https://about.me/lgatica)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lgaticaq/rut-status-cli/issues"
  },
  "homepage": "https://github.com/lgaticaq/rut-status-cli#readme",
  "dependencies": {
    "chalk": "^2.0.0",
    "commander": "^2.9.0",
    "ora": "^1.1.0",
    "rut-status": "^2.0.0",
    "update-notifier": "^2.0.0"
  },
  "devDependencies": {
    "eslint": "4.7.2"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "indent": [
        2,
        2
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "quotes": [
        2,
        "single"
      ],
      "semi": [
        2,
        "always"
      ],
      "no-console": [
        0
      ]
    }
  }
}
